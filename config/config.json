      "components": [
        {
          "entrypoint": "./lib/webparts/promotions/PromotionsWebPart.js",
          "manifest": "./src/webparts/promotions/PromotionsWebPart.manifest.json"
        }
      ]
    },
    "banner-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/banner/BannerWebPart.js",
          "manifest": "./src/webparts/banner/BannerWebPart.manifest.json"
        }
      ]
    },
    "our-performance-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/ourPerformance/OurPerformanceWebPart.js",
          "manifest": "./src/webparts/ourPerformance/OurPerformanceWebPart.manifest.json"
        }
      ]
    },
    "profile-web-part": {
      "components": [
