    "PromotionsWebPartStrings": "lib/webparts/promotions/loc/{locale}.js",
    "BannerWebPartStrings": "lib/webparts/banner/loc/{locale}.js",
    "OurPerformanceWebPartStrings": "lib/webparts/ourPerformance/loc/{locale}.js",
    "ProfileWebPartStrings": "lib/webparts/profile/loc/{locale}.js",
    "NavMenuWebPartStrings": "lib/webparts/navMenu/loc/{locale}.js",
    "InstgramFeedsWebPartStrings": "lib/webparts/instgramFeeds/loc/{locale}.js",
    "PollsWebPartStrings": "lib/webparts/polls/loc/{locale}.js"
  }
}
