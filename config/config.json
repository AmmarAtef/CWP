    "instgram-feeds-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/instgramFeeds/InstgramFeedsWebPart.js",
          "manifest": "./src/webparts/instgramFeeds/InstgramFeedsWebPart.manifest.json"
        }
      ]
    },
    "polls-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/polls/PollsWebPart.js",
          "manifest": "./src/webparts/polls/PollsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": "node_modules/jquery/dist/jquery.min.js",
    "jqueryui": "node_modules/jqueryui/jquery-ui.min.js"
  },
  "localizedResources": {
    "WeatherWebPartStrings": "lib/webparts/weather/loc/{locale}.js",
    "CalendarWebPartStrings": "lib/webparts/calendar/loc/{locale}.js",
    "QuickLinksWebPartStrings": "lib/webparts/quickLinks/loc/{locale}.js",
    "HotLineNumbersWebPartStrings": "lib/webparts/hotLineNumbers/loc/{locale}.js",
